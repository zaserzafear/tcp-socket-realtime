{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Console": {
      "FormatterName": "datetime",
      "FormatterOptions": {
        "TimestampFormat": "[yyyy-MM-dd HH:mm:ss] "
      }
    }
  },
  "AllowedHosts": "*",
  "TcpServerSetting": {
    "Host": "0.0.0.0",
    "EntryPort": 5000,
    "ServerPorts": [ 5001, 5002, 5003, 5004, 5005, 5006, 5007, 5008, 5009, 5010 ],
    "BufferSize": 1024,
    "BackLog": 100
  }
}
