{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Console": {
      "FormatterName": "datetime",
      "FormatterOptions": {
        "TimestampFormat": "[yyyy-MM-dd HH:mm:ss] "
      }
    }
  },
  "AllowedHosts": "*",
  "TcpServerSetting": {
    "Host": "0.0.0.0",
    "EntryPort": 5000,
    "ServerPorts": [ 5001, 5002, 5003 ]
  }
}
